<tile>
    <visual version="2">
        <binding template="TileSquare150x150Text01" fallback="TileSquareText01">
            <text id="1">°@ViewBag.Temp1</text>
            <text id="2">@ViewBag.Descr1</text>
            <text id="3">Sunrise: @ViewBag.Sunrise</text> 
            <text id="4">Sunset: @ViewBag.Sunset</text>
        </binding>
    </visual>
</tile>
